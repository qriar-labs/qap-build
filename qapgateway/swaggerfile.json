    "/api-banking/api-coingecko/simple/price": {
      "get": {
        "summary": "Get Simple Prices",
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "additionalProperties": {
                    "type": "object",
                    "properties": {
                      "usd": {
                        "type": "number",
                        "description": "Price in USD."
                      },
                      "usd_24h_vol": {
                        "type": "number",
                        "description": "24h trading volume in USD (if included)."
                      },
                      "usd_24h_change": {
                        "type": "number",
                        "description": "24h percentage change in USD (if included)."
                      },
                      "usd_market_cap": {
                        "type": "number",
                        "description": "Market cap in USD (if included)."
                      }
                    }
                  }
                }
              }
            },
            "description": "Successfully retrieved prices."
          },
          "400": {
            "description": "Invalid request parameters."
          }
        },
        "parameters": [
          {
            "in": "query",
            "name": "ids",
            "schema": {
              "type": "string",
              "example": "bitcoin,ethereum"
            },
            "required": true,
            "description": "IDs of the cryptocurrencies (e.g., 'bitcoin,ethereum')."
          },
          {
            "in": "query",
            "name": "vs_currencies",
            "schema": {
              "type": "string",
              "example": "usd,eur"
            },
            "required": true,
            "description": "Currencies to convert to (e.g., 'usd,eur')."
          },
          {
            "in": "query",
            "name": "include_market_cap",
            "schema": {
              "type": "boolean",
              "example": false
            },
            "required": false,
            "description": "Include market cap data (true/false)."
          },
          {
            "in": "query",
            "name": "include_24hr_vol",
            "schema": {
              "type": "boolean",
              "example": false
            },
            "required": false,
            "description": "Include 24h volume data (true/false)."
          },
          {
            "in": "query",
            "name": "include_24hr_change",
            "schema": {
              "type": "boolean",
              "example": false
            },
            "required": false,
            "description": "Include 24h change percentage (true/false)."
          }
        ],
        "description": "Retrieve current prices of cryptocurrencies in specified currencies."
      }
    },
    "/api-banking/api-open-library/subjects/{subject}.json": {
      "get": {
        "summary": "Get books by subject",
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "key": {
                      "type": "string",
                      "description": "Subject key."
                    },
                    "name": {
                      "type": "string",
                      "description": "Name of the subject."
                    },
                    "works": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "key": {
                            "type": "string",
                            "description": "Unique key for the work in Open Library."
                          },
                          "title": {
                            "type": "string",
                            "description": "Title of the work."
                          },
                          "authors": {
                            "type": "array",
                            "items": {
                              "type": "object",
                              "properties": {
                                "key": {
                                  "type": "string",
                                  "description": "Author's key."
                                },
                                "name": {
                                  "type": "string",
                                  "description": "Author's name."
                                }
                              }
                            },
                            "description": "List of authors."
                          },
                          "cover_id": {
                            "type": "integer",
                            "description": "ID of the cover image."
                          },
                          "edition_count": {
                            "type": "integer",
                            "description": "Number of editions available."
                          },
                          "first_publish_year": {
                            "type": "integer",
                            "description": "Year the work was first published."
                          }
                        }
                      }
                    },
                    "work_count": {
                      "type": "integer",
                      "description": "Number of works related to the subject."
                    }
                  }
                },
                "examples": {
                  "example-1": {
                    "value": {
                      "key": "science",
                      "name": "Science",
                      "works": [
                        {
                          "key": "/works/OL67890W",
                          "title": "A Brief History of Time",
                          "authors": [
                            {
                              "key": "/authors/OL12345A",
                              "name": "Stephen Hawking"
                            }
                          ],
                          "cover_id": 123456,
                          "edition_count": 45,
                          "first_publish_year": 1988
                        }
                      ],
                      "work_count": 1200
                    },
                    "summary": "Example response"
                  }
                }
              }
            },
            "description": "A list of books related to the specified subject."
          }
        },
        "parameters": [
          {
            "in": "path",
            "name": "subject",
            "schema": {
              "type": "string",
              "example": "science"
            },
            "required": true,
            "description": "The subject to retrieve books for (e.g., 'science')."
          },
          {
            "in": "query",
            "name": "limit",
            "schema": {
              "type": "integer",
              "example": 10
            },
            "required": false,
            "description": "Number of results to return."
          }
        ],
        "description": "Retrieve a list of books related to a specific subject."
      }
    }
  },
