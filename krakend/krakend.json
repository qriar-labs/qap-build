{
  "version": 3,
  "name": "user-api",
  "port": 8080,
  "endpoints": [
    {
      "endpoint": "user-api/objects",
      "method": "GET",
      "backend": [
        {
          "url_pattern": "/objects",
          "host": [
            "https://api.restful-api.dev"
          ]
        }
      ]
    },
    {
      "endpoint": "user-api/login",
      "method": "POST",
      "backend": [
        {
          "url_pattern": "/login",
          "host": [
            "http://localhost:3000"
          ]
        }
      ]
    }
  ]
}