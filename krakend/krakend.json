{
  "version": 3,
  "name": "api-pokemon",
  "timeout": "5000ms",
  "cache_ttl": "300s",
  "output_encoding": "no-op",
  "port": 3890,
  "endpoints": [
    {
      "endpoint": "/api-pokemon/pokemon/ditto",
      "method": "GET",
      "output_encoding": "no-op",
      "extra_config": {
        "security/cors": {
          "allow_origins": [
            "*"
          ],
          "allow_methods": [
            "GET",
            "HEAD",
            "POST"
          ],
          "expose_headers": [
            "Content-Length",
            "Content-Type"
          ],
          "allow_headers": [
            "Accept-Language"
          ],
          "max_age": "12h",
          "allow_credentials": false
        }
      },
      "backend": [
        {
          "url_pattern": "/api/v2/pokemon/ditto",
          "encoding": "no-op",
          "sd": "static",
          "method": "GET",
          "cache_ttl": "300s",
          "host": [
            "https://pokeapi.co"
          ],
          "disable_host_sanitize": false
        }
      ]
    }
  ]
}