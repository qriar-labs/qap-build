{
  "version": 2,
  "name": "user-api",
  "port": 8080,
  "endpoints": [
    {
      "endpoint": "/user-api/users/{userId}",
      "method": "GET",
      "backend": [
        {
          "url_pattern": "/users/{userId}",
          "host": [
            "http://localhost:3000"
          ]
        }
      ]
    },
    {
      "endpoint": "/user-api/login",
      "method": "POST",
      "backend": [
        {
          "url_pattern": "/login",
          "host": [
            "http://localhost:3000"
          ]
        }
      ]
    }
  ]
}